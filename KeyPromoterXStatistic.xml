<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>FileChooser.Refresh</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Refresh</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Tests in 'com.woowacourse''">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'Tests in 'com.woowacourse''</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>